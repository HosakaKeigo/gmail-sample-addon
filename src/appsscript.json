      },
      "layoutProperties": {
        "primaryColor": "#ff392b",
        "secondaryColor": "#d68617"
      },
      "logoUrl": "https://ssl.gstatic.com/docs/script/images/logo/script-64.png",
      "name": "Sample",
      "useLocaleFromApp": true
    },
    "gmail": {
      "composeTrigger": {
        "selectActions": [
          {
            "text": "テスト",
            "runFunction": "onComposeTrigger"
          }
        ],
        "draftAccess": "METADATA"
      },
      "contextualTriggers": [
        {
          "unconditional": {},
          "onTriggerFunction": "onGmailMessageOpen"
        }
      ]
    }
  }
